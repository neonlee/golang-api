{
    "swagger": "2.0",
    "info": {
        "description": "API para gerenciamento de petshop",
        "title": "API do Petshop",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/pets": {
            "get": {
                "description": "Retorna todos os pets cadastrados",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "pets"
                ],
                "summary": "Lista todos os pets",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/main.Pet"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.Pet": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "ID do pet",
                    "type": "integer",
                    "example": 1
                },
                "name": {
                    "description": "Nome do pet",
                    "type": "string",
                    "example": "Rex"
                },
                "type": {
                    "description": "Tipo do animal",
                    "type": "string",
                    "example": "Cachorro"
                }
            }
        }
    }
}